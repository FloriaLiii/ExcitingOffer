<!--pages/recommended/salary/salary.wxml-->
<view class="de-main">
	<view class="home-jcly">
		<view class="cu-item grayscale">
			<view class="cu-avatar radius ">
				<text>薪</text>
			</view>
			<view class="content2">
				<view>
					<text class="text-cut text-lg">薪资待遇</text>
				</view>
				<view class="text-gray text-sm flex">
					<text class="text-cut"></text>
				</view>
			</view>
		</view>

		<view class="detail-tl">
			{{ salary_list.company}}
		</view>
	</view>
	<view class="m3">
		<view class="tille-1">
			薪资详情
		</view>
    <view class="detil">
			<text>行业信息:  {{salary_list.industry}}</text> 
		</view>
    <view class="detil">
			<text>岗位信息:  {{salary_list.position}}</text> 
		</view>
		<view class="detil">
			<text>工资详情:  {{salary_list.salary}}</text> 
		</view>
    <view class="detil">
			<text>可信度:  {{salary_list.confidence}}</text> 
		</view>
	</view>
</view>


